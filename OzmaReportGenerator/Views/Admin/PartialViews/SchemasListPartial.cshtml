@model List<ReportGenerator.Models.ReportTemplateSchema>
<thead>
<tr>
    <th>Название</th>
    <th></th>
</tr>
</thead>
<tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td><button type="button" class="btn btn-danger" onclick="Delete('Schema', @item.Id); ">Удалить</button></td>
        </tr>
    }
</tbody>