@model List<ReportGenerator.Models.VReportTemplate>
<thead>
    <tr>
        <th>Название</th>
        <th>Схема</th>
        <th></th>
        <th></th>
    </tr>
</thead>
<tbody>
@foreach (var item in Model)
{
    <tr>
        <td>@item.Name</td>
        <td>@item.Schema.Name</td>
        <td><button type="button" class="btn btn-success" onclick="SelectOdt(@item.Id) ">Обновить файл шаблона</button></td>
        <td><button type="button" class="btn btn-danger" onclick="Delete('Template', @item.Id) ">Удалить</button></td>
    </tr>
}
</tbody>
